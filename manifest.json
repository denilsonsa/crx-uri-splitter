{
  "manifest_version": 2,

  "name": "URI Splitter",
  "short_name": "URI Splitter",
  "description": "Easily and quickly edit the URL/URI from the current page",
  "author": "Denilson SÃ¡ Maia",
  "version": "0.95",
  "homepage_url": "https://github.com/denilsonsa/crx-uri-splitter/",

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "24": "icon24.png",
      "32": "icon32.png"
    },
    "default_popup": "popup.html",
    "default_title": "URI Splitter"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      }
    }
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "incognito": "spanning",
  "offline_enabled": true,
  "permissions": [
    "activeTab",
    "storage"
  ]
}
